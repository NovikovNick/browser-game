      webSocket.activate();\n\n        document.addEventListener('mousemove', onMouseUpdate, false);\n        document.addEventListener('mouseenter', onMouseUpdate, false);\n    })\n\n    return (\n        <div/>\n    );\n}\n\nconst mapDispatchToProps = dispatch => ({\n    actions: bindActionCreators(Store, dispatch)\n});\nexport default connect(null, mapDispatchToProps)(WebSocket);\n","/home/nicknovikov/WORKSPACE/small-test-server/frontend/src/store/reducers/players.js",[],"/home/nicknovikov/WORKSPACE/small-test-server/frontend/src/container/Controls.js",[],"/home/nicknovikov/WORKSPACE/small-test-server/frontend/src/service/service.js",[],"/home/nicknovikov/WORKSPACE/small-test-server/frontend/src/config.js",[],{"ruleId":"57","replacedBy":"58"},{"ruleId":"59","replacedBy":"60"},{"ruleId":"61","severity":1,"message":"62","line":23,"column":11,"nodeType":"63","messageId":"64","endLine":23,"endColumn":18},{"ruleId":"61","severity":1,"message":"65","line":63,"column":5,"nodeType":"63","messageId":"64","endLine":63,"endColumn":12},{"ruleId":"61","severity":1,"message":"65","line":51,"column":21,"nodeType":"63","messageId":"64","endLine":51,"endColumn":28},"no-native-reassign",["66"],"no-negated-in-lhs",["67"],"no-unused-vars","'session' is assigned a value but never used.","Identifier","unusedVar","'timerId' is assigned a value but never used.","no-global-assign","no-unsafe-negation"]